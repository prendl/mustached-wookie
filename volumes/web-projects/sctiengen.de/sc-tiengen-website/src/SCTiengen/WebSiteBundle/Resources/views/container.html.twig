{% block container %}
  {% for childBlock in block.children %}
    {% set blockType = childBlock.subtype|default('') %}
    {% if childBlock.name in ['page-head', 'submenu'] %}
      {% set blockType = childBlock.name %}
    {% endif %}
    {%if blockType is empty %}
        {{ sonata_block_render(childBlock) }}
    {% else %}
        {{ sonata_block_render(childBlock, {'template': 'SCTiengenWebSiteBundle:Blocks:' ~ blockType ~ '.html.twig'}) }}
    {% endif %}
        {#{ sonata_block_render(childBlock, {'template': 'SCTiengenWebSiteBundle:Blocks:' ~ childBlock.name|split('_')|first ~ '.html.twig'}) }#}
  {% endfor %}
{% endblock %}
